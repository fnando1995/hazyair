{
  "name": "hazyair",
  "version": "1.1.0",
  "description": "Dust concentration and temperature monitoring tool.",
  "main": "hazyair.js",
  "scripts": {
    "postinstall": "cd public && npm install && sudo npm install pm2 -g",
    "plantower": "sudo ./scripts/plantower.sh",
    "maxim": "sudo ./scripts/maxim.sh",
    "bosh": "sudo ./scripts/bosh.sh",
    "deploy": "pm2 start server.js && pm2 save && pm2 startup | grep -v PM2 | sh",
    "undeploy": "pm2 stop server && pm2 delete server && pm2 save && pm2 unstartup | grep -v PM2 | sh",
    "start": "pm2 start server.js",
    "stop": "pm2 stop server",
    "restart": "pm2 restart server",
    "delete": "pm2 delete server",
    "show": "pm2 show server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcin-sielski/hazyair.git"
  },
  "keywords": [
    "pm1.0",
    "pm2.5",
    "pm10",
    "dust monitoring",
    "temperature monitoring",
    "humidity monitoring",
    "pressure monitoring",
    "air quality",
    "weather",
    "monitor",
    "dust",
    "tempreature",
    "humidity",
    "pressure",
    "weather station"
  ],
  "author": "Marcin Sielski",
  "license": "MIT",
  "dependencies": {
    "berkeleydb": "^0.2.1",
    "bme280-sensor": "^0.1.6",
    "cron": "^1.3.0",
    "ds18b20": "^0.1.0",
    "express": "^4.16.2",
    "moment": "^2.20.1",
    "plantower": "^1.1.6",
    "yargs": "^10.0.3"
  }
}
